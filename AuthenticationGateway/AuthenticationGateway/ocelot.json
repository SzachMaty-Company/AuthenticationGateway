  "Routes": [
    {
      "DownstreamPathTemplate": "/registerws",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "chatservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/registerws",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/registerws/info",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "chatservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/registerws/info",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/topic/chat",
      "DownstreamScheme": "ws",
      "DownstreamHostAndPorts": [
        {
          "Host": "chatservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/topic/chat",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/app/chat",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "chatservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/app/chat",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/auth/google",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "user_data_service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/auth/google",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "JwtBearer",
        "AllowedScopes": []
      },
      "DownstreamPathTemplate": "/auth/protected",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "user_data_service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/protected",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://gateway:80",
    "WebSocketOptions": {
      "KeepAliveInterval": 20,
      "AllowedOrigins": [ "*" ] // Zmodyfikuj na zgodnie z wymaganiami bezpiecze≈Ñstwa
    }
  }
}